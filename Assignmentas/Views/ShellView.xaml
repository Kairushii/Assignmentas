<Window x:Class="Assignmentas.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignmentas.Views"
        mc:Ignorable="d"
       Background="Transparent" AllowsTransparency="True" FontFamily="Montserrat"
        Title="TimeInOut" Height="600" Width="800" WindowStyle="None" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
        <Window.Clip>
            <RectangleGeometry Rect="0,0,800,600" RadiusX="25" RadiusY="25"/>
        </Window.Clip>
        <Grid x:Name="GridOfWindow" MouseLeftButtonDown="GridOfWindow_MouseLeftButtonDown">
            <ContentControl x:Name="ActiveItem" Margin="0 0 0 0" />
        </Grid>
   

</Window>
